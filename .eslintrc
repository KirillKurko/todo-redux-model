{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "root": true,
  "parser": "@typescript-eslint/parser",
